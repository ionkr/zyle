{
  "name": "zyle",
  "version": "0.1.1",
  "description": "A zero-dependency library that embeds into web frontends to capture and analyze console logs with AI-powered insights",
  "type": "module",
  "main": "./dist/zyle.umd.js",
  "module": "./dist/zyle.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/zyle.es.js",
      "require": "./dist/zyle.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm publish",
    "prepublishOnly": "pnpm typecheck"
  },
  "keywords": [
    "console",
    "log",
    "analyzer",
    "debug",
    "sourcemap",
    "error-tracking"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "typescript": "^5.3.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0"
  }
}
